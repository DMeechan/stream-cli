{
	"name": "getstream-cli",
	"description": "Configure & manage Stream applications from the command line. ðŸš€",
	"version": "0.0.54",
	"bin": {
		"stream": "./bin/run"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"main": "src/index.js",
	"author": {
		"name": "Nick Parsons",
		"email": "nick@getstream.io",
		"url": "https://getstream.io/"
	},
	"bugs": {
		"url": "https://github.com/getstream/stream-cli/issues",
		"email": "support@getstream.io"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"homepage": "https://github.com/GetStream/stream-cli/blob/master/README.md",
	"keywords": [
		"GetStream",
		"Stream",
		"CLI",
		"Feed",
		"Chat"
	],
	"license": "BSD-3-Clause",
	"scripts": {
		"manifest": "rm -f oclif.manifest.json && oclif-dev manifest",
		"readme": "oclif-dev readme --multi",
		"test": "nyc report --reporter=text-lcov | coveralls mocha --exit --forbid-only \"test/**/*.test.js\"",
		"lint": "yarn prettier --write --check --config ./.prettierrc \"**/*.js\" && eslint --fix ./src",
		"build": "./build.sh",
		"pre-release": "git pull origin master",
		"mid-release": "git pull origin master && yarn manifest && yarn readme && yarn lint && git add . && git pull origin master && git commit -m 'New Release' && git push origin master",
		"post-release": "yarn build",
		"release": "release-it"
	},
	"dependencies": {
		"@babel/runtime": "^7.7.2",
		"@oclif/command": "^1",
		"@oclif/config": "^1",
		"@oclif/plugin-autocomplete": "^0.1.4",
		"@oclif/plugin-commands": "^1.2.3",
		"@oclif/plugin-help": "^2.2.1",
		"@oclif/plugin-not-found": "^1.2.3",
		"@oclif/plugin-warn-if-update-available": "^1.7.0",
		"cardinal": "^2.1.1",
		"chalk": "^3.0.0",
		"cli-table": "^0.3.1",
		"data-store": "^4.0.3",
		"dotenv": "^8.2.0",
		"enquirer": "^2.3.2",
		"escape-json-node": "^2.0.0",
		"fs-extra": "^8.1.0",
		"globby": "^10.0.1",
		"json-stringify-pretty-compact": "^2.0.0",
		"jsonwebtoken": "^8.5.1",
		"md5": "^2.2.1",
		"moment": "^2.24.0",
		"node-emoji": "^1.10.0",
		"node-notifier": "^6.0.0",
		"numeral": "^2.0.6",
		"rollbar": "^2.14.4",
		"stream-chat": "1.1.7",
		"uuid": "^3.3.3"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1",
		"@oclif/test": "^1",
		"babel-eslint": "^10.0.3",
		"chai": "^4",
		"coveralls": "^3.0.7",
		"eslint": "^6.6",
		"eslint-config-oclif": "^3.1",
		"eslint-plugin-babel": "^5.3.0",
		"husky": "^3.0.9",
		"mocha": "^6.2.2",
		"mocha-lcov-reporter": "^1.3.0",
		"nyc": "^14",
		"prettier": "^1.19.1",
		"release-it": "^12.4.3"
	},
	"files": [
		"/bin",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json",
		"/src"
	],
	"oclif": {
		"bin": "stream",
		"commands": "./src/commands",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-autocomplete",
			"@oclif/plugin-commands",
			"@oclif/plugin-not-found",
			"@oclif/plugin-warn-if-update-available"
		],
		"warn-if-update-available": {
			"timeoutInDays": 7,
			"message": "NOTICE: <%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>!"
		},
		"hooks": {
			"telemetry": "./src/utils/hooks/telemetry"
		}
	}
}

{
    "name": "getstream-cli",
    "description": "The Stream Command Line Interface (CLI) makes it easy to create and manage your Stream apps directly from the terminal.",
    "version": "0.0.1-beta.23",
    "author": "Nick Parsons <nparsons08@gmail.com>",
    "bin": {
        "stream": "./bin/run"
    },
    "bugs": "https://github.com/getstream/stream-cli/issues",
    "repository": "getstream/stream-cli",
    "homepage": "https://github.com/getstream/stream-cli",
    "keywords": [
        "GetStream",
        "Stream",
        "CLI",
        "Feed",
        "Chat"
    ],
    "license": "BSD-3-Clause",
    "main": "src/index.js",
    "scripts": {
        "prepack": "rm -f oclif.manifest.json && oclif-dev manifest",
        "posttest": "eslint .",
        "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
        "readme": "oclif-dev readme --multi"
    },
    "dependencies": {
        "@oclif/command": "^1",
        "@oclif/config": "^1",
        "@oclif/plugin-autocomplete": "^0.1.0",
        "@oclif/plugin-commands": "^1.2.2",
        "@oclif/plugin-help": "^2.1.6",
        "@oclif/plugin-not-found": "^1.2.2",
        "@oclif/plugin-warn-if-update-available": "^1.7.0",
        "axios": "^0.18.0",
        "cardinal": "^2.1.1",
        "chalk": "^2.4.2",
        "cli-table": "^0.3.1",
        "data-store": "^3.1.0",
        "enquirer": "^2.3.0",
        "globby": "^9.0.0",
        "json-stringify-pretty-compact": "^2.0.0",
        "jsonwebtoken": "^8.5.0",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "node-emoji": "^1.10.0",
        "node-fs-extra": "^0.8.2",
        "node-notifier": "^5.4.0",
        "numeral": "^2.0.6",
        "stream-chat": "^0.3.1",
        "treeify": "^1.1.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@babel/runtime": "^7.3.1",
        "@oclif/dev-cli": "^1",
        "@oclif/test": "^1",
        "aws-sdk": "^2.408.0",
        "chai": "^4",
        "eslint": "^5.14",
        "eslint-config-oclif": "^3.1",
        "mocha": "^6.0.1",
        "nyc": "^13",
        "prettier": "^1.16.4"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "files": [
        "/bin",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json",
        "/src"
    ],
    "oclif": {
        "bin": "stream",
        "commands": "./src/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-autocomplete",
            "@oclif/plugin-commands",
            "@oclif/plugin-not-found",
            "@oclif/plugin-warn-if-update-available"
        ],
        "warn-if-update-available": {
            "timeoutInDays": 7,
            "message": "NOTICE: <%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>!"
        },
        "topics": {
            "config": {
                "description": "manage configuration variables"
            },
            "debug": {
                "description": "debugging tools for the command line"
            },
            "debug:token": {
                "description": "debug stream jwt tokens on the fly"
            },
            "chat": {
                "description": "configure and manage all things related to chat"
            },
            "chat:channel": {
                "description": "get and manage configurations for a channel"
            },
            "chat:channel:get": {
                "description": "get details from a specific channel"
            },
            "chat:channel:edit": {
                "description": "edit configuration for a channel"
            },
            "chat:channel:init": {
                "description": "create a new channel"
            },
            "chat:channel:list": {
                "description": "get all channels"
            },
            "chat:channel:query": {
                "description": "query all channels"
            },
            "chat:log": {
                "description": "log events from a channel"
            },
            "chat:message": {
                "description": "send and remove messages from a channel"
            },
            "chat:message:remove": {
                "description": "remove a message from a channel"
            },
            "chat:message:send": {
                "description": "send a new message with our without attachments"
            },
            "chat:moderate": {
                "description": "moderatation tools for a channel"
            },
            "chat:moderate:ban": {
                "description": "ban a user temporarily or based on a per minute timeout"
            },
            "chat:moderate:flag": {
                "description": "flag a user for innapropriate comments or content"
            },
            "chat:moderate:mute": {
                "description": "silence a user within a channel"
            },
            "chat:user": {
                "description": "manage users within channels"
            },
            "chat:user:add": {
                "description": "add a user to a channel"
            },
            "chat:user:remove": {
                "description": "remove a user from a channel"
            }
        }
    }
}

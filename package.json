{
    "name": "getstream-cli",
    "description": "The Stream Command Line Interface (CLI) makes it easy to create and manage your Stream apps directly from the terminal.",
    "version": "0.0.1-beta.15",
    "author": "Nick Parsons <nparsons08@gmail.com>",
    "bin": {
        "stream": "./bin/run"
    },
    "bugs": "https://github.com/getstream/stream-cli/issues",
    "repository": "getstream/stream-cli",
    "homepage": "https://github.com/getstream/stream-cli",
    "keywords": [
        "GetStream",
        "Stream",
        "CLI",
        "Feed",
        "Chat"
    ],
    "license": "BSD-3-Clause",
    "main": "src/index.js",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint .",
        "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
        "version": "oclif-dev readme && git add README.md",
        "release:manifest": "oclif-dev manifest",
        "release:macos": "oclif-dev pack:macos && oclif-dev publish:macos",
        "release:pack": "oclif-dev pack",
        "release:publish": "oclif-dev publish",
        "release:npm": "npm version patch && npm publish",
        "release": "yarn release:pack && yarn release:publish"
    },
    "dependencies": {
        "@oclif/command": "^1",
        "@oclif/config": "^1",
        "@oclif/plugin-autocomplete": "^0.1.0",
        "@oclif/plugin-commands": "^1.2.2",
        "@oclif/plugin-help": "^2.1.6",
        "@oclif/plugin-not-found": "^1.2.2",
        "@oclif/plugin-update": "^1.3.9",
        "@oclif/plugin-warn-if-update-available": "^1.7.0",
        "cardinal": "^2.1.1",
        "chalk": "^2.4.2",
        "cli-table": "^0.3.1",
        "enquirer": "^2.3.0",
        "globby": "^9.0.0",
        "json-stringify-pretty-compact": "^2.0.0",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "node-emoji": "^1.10.0",
        "node-fs-extra": "^0.8.2",
        "node-notifier": "^5.4.0",
        "numeral": "^2.0.6",
        "opn": "^5.4.0",
        "stream-chat": "^0.3.1",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1",
        "@oclif/test": "^1",
        "aws-sdk": "^2.408.0",
        "chai": "^4",
        "eslint": "^5.14",
        "eslint-config-oclif": "^3.1",
        "mocha": "^6.0.1",
        "nyc": "^13",
        "prettier": "^1.16.4"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "files": [
        "/bin",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json",
        "/src"
    ],
    "oclif": {
        "bin": "stream",
        "commands": "./src/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-autocomplete",
            "@oclif/plugin-commands",
            "@oclif/plugin-not-found",
            "@oclif/plugin-update",
            "@oclif/plugin-warn-if-update-available"
        ],
        "warn-if-update-available": {
            "timeoutInDays": 7
        },
        "macos": {
            "identifier": "io.getsream.cli"
        },
        "update": {
            "s3": {
                "bucket": "stream-cli"
            }
        },
        "topics": {
            "config": {
                "description": "manage config variables"
            },
            "chat": {
                "description": "configure and manage all things related to chat"
            },
            "chat:channel": {
                "description": "create, get, query, and edit a channel"
            },
            "chat:log": {
                "description": "log events from a channel based on an event type"
            },
            "chat:message": {
                "description": "send and remove messages from a channel"
            },
            "chat:moderate": {
                "description": "moderate a channel"
            },
            "chat:user": {
                "description": "manage users within channels"
            }
        }
    }
}
